//------------------- Task 1.4 -------------------//

db.cl.aggregate([
{
    $match: {
    	statusesCount: { $gt: 200, $lt: 203}
    }
},
{
    $project: {
        _id: 0,
        displayName: 1,
        followersCount: 1
    }
}])
